<!-- Cards -->

<div id="cards">
 
  <div class="row">
    {% for card in dataCards %}
        <div class="card col-md-12 col-lg-3">
          <div class="card col-12" style="background-image: url({{ card.image }}); background-size:cover; height:200px;"></div>
          <div class="card-body">
            <h5 class="card-title">{{ card.title }} </h5>
            <p class="card-text">{{ card.text }}</p>
          </div>
        </div>
    {% endfor %}
  </div>

</div>
